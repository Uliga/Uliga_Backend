<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 위 코드가 mapper 설정을 하는 코드 -->

<!-- Mapper를 사용하는 Repository의 전체 경로를 적어줘야함. -->
<mapper namespace="com.uliga.uliga_backend.domain.Member.dao.MemberMapperRepository">
<select id="find" resultType="com.uliga.uliga_backend.domain.Member.dto.NativeQ.MemberInfoNativeQ">
    select (member.member_id, account_book_id, avatar_url, user_name, nick_name, email)
    FROM uliga_db.member JOIN member_base mb on member.member_id = mb.member_id
</select>
</mapper>